<script>
function selectAddress(select) {
    fetch('/selectAddress',{
        method:"post",
        headers:{
                'Content-Type':'application/json'

        },body:JSON.stringify({select})
    })
}






// Function to show the alert and hide it after 1 minute
function showAlertAndHide() {
    var alertDiv = document.getElementById('orderAlert');
    alertDiv.style.display = 'block'; // Show the alert

    setTimeout(function() {
        alertDiv.style.display = 'none'; // Hide the alert after 1 minute
    }, 60000); // 1 minute = 60,000 milliseconds
}

// Add event listener to the "Place Order" button
document.getElementById('placeOrderBtn').addEventListener('click', showAlertAndHide);


</script>
