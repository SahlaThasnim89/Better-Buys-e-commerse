      function razor(amount, userId) {

          fetch('/razorPay', {
              method: 'POST',
              headers: { 'Content-type': 'application/json' },
              body: JSON.stringify({ amount, userId})
          }).then(res => res.json()).then(data => {
              if (data.succes) {
                  console.log(data)
                  let options = {
                      "key": `${data.key_id}`,
                      "amount":` ${data.amount}`,
                      "currency": "INR",
                      "name": "TORO",
                      "order_id": `${data.order_id}`,
                      "handler": function (response) {
                          form.submit();
                          const Toast = Swal.mixin({
    toast: true,
    position: "bottom",
    showConfirmButton: false,
    timer: 1500,
    customClass: {
        popup: 'swal-toast',
        title: 'swal-toast-title'
    },
    didOpen: (toast) => {
        toast.style.backgroundColor = "black"; // Set background color to black
        toast.style.color = "white"; // Set text color to white
        toast.style.fontSize = "11px"; // Set font size to 11px
    }
});

Toast.fire({
    text: "Order placed successfully"
});

                      },
                      "profile": {
                          "name": `${data.FullName}`,
                          "email": `${data.email}`
                      }
                  }

                  let razorpayObject = new Razorpay(options);
                  razorpayObject.on('payment.failed', (response) => {
                      alert('payment failed');
                      Swal.fire({
                     position: "bottom",
                    title: "Payment Failed",
                     showConfirmButton: false,
                     timer: 1500
                    });
                  });
                  razorpayObject.open();
              }else if(data.fail){

                const Toast = Swal.mixin({
    toast: true,
    position: "bottom",
    showConfirmButton: false,
